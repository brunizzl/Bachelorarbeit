\renewcommand*{\inittitle}{%
\ifstsreprt@grayscaletitle
\definecolor{keycolor1}{gray}{.5}
  \ifx\@titlehead\@empty
    \titlehead{\hspace*{\fill}\includegraphics[width=55mm,height=16mm]{logo/logo_tuhh_gray}
  }%
  \fi
 \ifx\@mainlogo\@empty
        \mainlogo{%
		\includegraphics[height=23mm]{logo/logo_sts_gray}
        }%
  \fi
\else
\definecolor{keycolor1}{RGB}{88, 190, 184} % cyan
\definecolor{keycolor1}{RGB}{80,118,179} % sts-logo-blue
  \ifx\@titlehead\@empty
    \titlehead{\hspace*{\fill}\includegraphics[width=55mm,height=16mm]{logo/logo_tuhh}
  }%
  \fi
 \ifx\@mainlogo\@empty
        \mainlogo{%
		\includegraphics[height=23mm]{logo/logo_sts}
        }%
\fi
\fi
  \ifx\@university\@empty
    \university{%
      Technische Universit\"at Hamburg (TUHH)\\
{\textsl{Hamburg University of Technology}}
    }%
  \fi
  \ifx\@faculty\@empty
    \faculty{%
      Institut für Zuverlässiges Rechnen
    }%
  \fi
  \ifx\@place\@empty
    \place{%
      21073 Hamburg
    }%
  \fi
  \ifx\@titlepagefont\@empty
    \renewcommand*{\@titlepagefont}{\sffamily}%
  \fi
}
